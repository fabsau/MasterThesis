# SentinelOne Management Console URL
S1_MANAGEMENT_URL=https://your-sentinelone-management-url

# SentinelOne API Token
S1_API_TOKEN=your-sentinelone-api-token

# Database Host (e.g. localhost or IP address)
DB_HOST=localhost

# Database Port (default for PostgreSQL is 5432)
DB_PORT=5432

# Database Name
DB_NAME=your-database-name

# Database Username
DB_USER=your-database-username

# Database Password
DB_PASSWORD=your-database-password